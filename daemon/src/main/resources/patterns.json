{
  "Pattern.Undefined":{
    "comment": "Normally, this situation is impossible.",
    "priority": 0
  },

  "Pattern.Port":{
    "srcport": "^[0-9]{1,5}$",
    "priority": 10
  },

  "Pattern.IP":{
    "dstip": "^[0-9]{1,3}.[0-9]{1,3}.[0-9]{1,3}.[0-9]{1,3}$",
    "priority": 20
  },

  "Pattern.Process":{
    "pid": "^[0-9]{1,5}$",
    "priority": 30
  },

  "Pattern.Yandex":{
    "comment": "data leaks somewhere to Yandex internals",
    "orgname": ".*yandex.*",
    "priority": 40
  },

  "Pattern.Google":{
    "comment": "data leaks somewhere to Google internals",
    "orgname": ".*google.*",
    "priority": 40
  },

  "Pattern.Amazon":{
    "orgname": ".*Amazon\\.Com.*",
    "priority": 40
  },

  "Pattern.VKontakte":{
    "orgname": ".*VKontakte\\sLtd.*",
    "priority": 40
  },

  "Pattern.Mailru":{
    "orgname": ".*Mail\\.Ru.*",
    "priority": 40
  },

  "Pattern.StdApps": {
    "comment": "This pattern is also used by selector UnusualAppSelector.java",
    "related-patterns": "Pattern\\.StdApp\\.([a-zA-Z0-9]+)$",
    "relation-mode": "any",
    "priority": 41
  },

  "Pattern.Amazon.Aws":{
    "related-patterns": "Pattern\\.(Amazon)",
    "relation-mode": "all",
    "hostname": ".*amazonaws.*",
    "orgname": ".*Amazon\\.Com.*",
    "priority": 50
  },

  "Pattern.Google.Cache":{
    "related-patterns": "Pattern\\.(Google)",
    "relation-mode": "all",
    "hostname": ".*cache.google.com$",
    "priority": 50
  },

  "Pattern.StdApp.Skype":{
    "processname": ".*skype.*"  ,
    "priority": 51
  },

  "Pattern.StdApp.Steam":{
    "processname" : ".*steam.*"  ,
    "orgname": ".*Valve\\sCorporation$",
    "priority": 51
  },

  "Pattern.StdApp.TorrentClient":{
    "processname" : ".*torrent.*"  ,
    "priority": 51
  },

  "Pattern.StdApp.Dropbox":{
    "processname" : "^dropbox.*"  ,
    "orgname": ".*Dropbox.*",
    "priority": 51
  },

  "Pattern.StdApp.Browser":{
    "comment": "Only top-5 known browsers",
    "processname" : ".*(chrome|firefox|mozilla|iexplore|opera|safari|browser|iceweasel|amigo).*"  ,
    "priority": 51
  },

  "Pattern.Microsoft.Updates":{
    "processname": "^svchost.*",
    "orgname": ".*Microsoft\\sCorporation.*",
    "priority": 52
  },

  "Pattern.StdApp.Browser.Google":{
