    "priority": 60
  },

  "Pattern.StdApp.Browser.VKontakte":{
    "related-patterns": "Pattern\\.StdApp\\.(Browser)",
    "relation-mode": "all",
    "orgname": ".*VKontakte\\sLtd.*",
    "priority": 60
  },

  "Pattern.StdApp.Browser.Yandex.Statica":{
